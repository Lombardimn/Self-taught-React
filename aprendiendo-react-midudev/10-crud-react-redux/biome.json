{
	"$schema": "https://biomejs.dev/schemas/1.0.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"a11y": {
				"useAltText": "warn"
			}
		}
	},
		"formatter": {
			"enabled": true
		},
	"javascript": {
		"formatter": {
		"jsxQuoteStyle": "single"
		}
	}
}
